
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet" href="../../css/table.css" type="text/css">
    <script type="text/javascript" src="../../javascript/comm/comm.js"></script>
    <script src="~/Scripts/vue/dist/vue.js"></script>
    <script src="~/Scripts/element-ui/lib/index.js"></script>
    <link href="~/Scripts/element-ui/lib/theme-chalk/index.css" rel="stylesheet" />
    <script src="~/Scripts/axios/dist/axios.js"></script>

    <script type="text/javascript">
        function check() {
            if (document.getElementById("publicChar.attributeName").value == "") {
                alert("薪酬项目名称不能为空!!!");
                return false;
            }
            document.forms[0].submit();
        }

    </script>
</head>
<body>
    <div id="app">
        <form action="" method="post" v-on:submit="SubTianTwo()">
            <table width="100%">
                <tbody>
                    <tr>
                        <td>
                            <font color="black">
                                您正在做的业务是：人力资源--客户化设置--薪酬管理设这--薪酬项目设置--薪酬项目添加
                            </font>
                        </td>
                    </tr>
                    <tr>
                        <td align="right">
                            <input type="submit" value="提交" class="BUTTON_STYLE1"  style="width: 65px; height: 30px; border-radius: 5px">
                            <input type="button" value="返回" class="BUTTON_STYLE1" onclick="history.back();"  style="width: 65px; height: 30px; border-radius: 5px">
                        </td>
                    </tr>
                </tbody>
            </table>

            <table width="100%" border="1" cellpadding="0" cellspacing="1" bordercolorlight="#848284" bordercolordark="#eeeeee" class="TABLE_STYLE1">
                <tbody>
                    <tr>
                        <td class="TD_STYLE1" width="19%">
                            薪酬项目名称
                        </td>
                        <td class="TD_STYLE2" width="81%">
                            <input type="text" class="INPUT_STYLE1" name="ssd.itemName" v-model="SubTian">
                        </td>
                    </tr>
                </tbody>
            </table>
        </form>
    </div>

    <script type="text/javascript">
        var vm = new Vue({
            el: "#app",
            data() {
                return {
                    SubTian: "",
                }
            }, methods: {
                SubTianTwo() {
                    axios.get("/Cpchar/Intset", {
                        params: {
                            attribute_name: this.SubTian
                        }
                    }).then(res => {
                        if (res.data == "1") {
                            location.href = '/Cpchar/PayIndex';
                            alert("增加成功");
                        } else {
                            alert("增加失败1")
                        }
                    }).catch(err => {
                        console.log(err);
                    })
                }
            }
        })
    </script>
</body>
</html>